                                },
                                "to": [{"key_code": "delete_or_backspace"} ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "9",
                                    "modifiers": {"mandatory": ["left_shift", "left_command", "left_control", "left_option"], "optional": ["any"] }
                                },
                                "to": [{"key_code": "delete_forward"} ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Make hyper + y, p, 7, 0  -->  [, ], {, }",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "y",
                                    "modifiers": {"mandatory": ["left_shift", "left_command", "left_control", "left_option"], "optional": ["any"] }
                                },
                                "to": [{"key_code": "8", "modifiers": ["right_option"] } ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "p",
                                    "modifiers": {"mandatory": ["left_shift", "left_command", "left_control", "left_option"], "optional": ["any"] }
                                },
                                "to": [{"key_code": "9", "modifiers": ["right_option"] } ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "7",
                                    "modifiers": {"mandatory": ["left_shift", "left_command", "left_control", "left_option"], "optional": ["any"] }
                                },
                                "to": [{"key_code": "8", "modifiers": ["right_option", "right_shift"] } ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "0",
                                    "modifiers": {"mandatory": ["left_shift", "left_command", "left_control", "left_option"], "optional": ["any"] }
                                },
                                "to": [{"key_code": "9", "modifiers": ["right_option", "right_shift"] } ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Make hyper + h, ö, ä, ¨ -->  (, ), /, ~",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "quote",
                                    "modifiers": {"mandatory": ["left_shift", "left_command", "left_control", "left_option"], "optional": ["any"] }
                                },
                                "to": [{"key_code": "7", "modifiers": ["right_shift"] } ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "semicolon",
                                    "modifiers": {"mandatory": ["left_shift", "left_command", "left_control", "left_option"], "optional": ["any"] }
                                },
                                "to": [{"key_code": "9", "modifiers": ["right_shift"] } ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "modifiers": {"mandatory": ["left_shift", "left_command", "left_control", "left_option"], "optional": ["any"] },
                                    "key_code": "h"                                    
                                },
                                "to": [{"key_code": "8", "modifiers": ["right_shift"] } ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "close_bracket",
                                    "modifiers": {"mandatory": ["left_shift", "left_command", "left_control", "left_option"], "optional": ["any"] }
                                },
                                "to": [{"key_code": "close_bracket", "modifiers": ["right_alt"] }, {"key_code": "spacebar"}],
                                "type": "basic"
                            }              
                        ]
                    },
                    {
                        "description": "Make caps into command except for ctrl in terminal",
                        "manipulators": [                            
                            {
                                "conditions": [{
                                    "bundle_identifiers": ["^com\\.apple\\.Terminal$"],
                                    "type": "frontmost_application_if"
                                } ],
                                "from": {"key_code": "caps_lock", "modifiers": {"optional": ["any"] } },
                                "to": [{"key_code": "left_control"} ],
                                "type": "basic"
                            },
                            {
                                "from": {"key_code": "caps_lock", "modifiers": {"optional": ["any"] }}, 
                                "to": [{"key_code": "right_command", "modifiers": [] } ],
                                "type": "basic"
                            }                            
                        ]
                    },
                    {
                        "description": "Make caps+tab function like ctrl+tab in chrome",
                        "manipulators": [
