                                "conditions": [{
                                    "bundle_identifiers": ["^com\\.apple\\.Terminal$", "^com\\.googlecode\\.iterm2$", "^co\\.zeit\\.hyperterm$", "^co\\.zeit\\.hyper$", "^io\\.alacritty$", "^net\\.kovidgoyal\\.kitty$"],
                                    "type": "frontmost_application_if"
                                }],
                                "from": {"key_code": "delete_forward", "modifiers": {"mandatory": ["command"] } },
                                "to": [{"key_code": "escape"}, {"key_code": "d"} ],
                                "type": "basic"
                            },                            
                            {
                                "from": {"key_code": "delete_forward", "modifiers": {"mandatory": ["command"] } },
                                "to": {"key_code": "delete_forward", "modifiers": ["right_option"] },
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "command (=capslock) + k | Delete to line end",
                        "manipulators": [
                            {
                                "conditions": [{
                                    "bundle_identifiers": ["^com\\.apple\\.Terminal$", "^com\\.googlecode\\.iterm2$", "^co\\.zeit\\.hyperterm$", "^co\\.zeit\\.hyper$", "^io\\.alacritty$", "^net\\.kovidgoyal\\.kitty$"],
