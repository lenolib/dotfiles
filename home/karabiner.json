                                    "type": "frontmost_application_if"
                                }],
                                "from": {"key_code": "left_arrow", "modifiers": {"mandatory": ["option"] } },
                                "to": [{"key_code": "escape"}, {"key_code": "b"} ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "⌥ + → | Move cursor one word ahead",
                        "manipulators": [
                            {
                                "conditions": [{
                                    "bundle_identifiers": ["^com\\.apple\\.Terminal$", "^com\\.googlecode\\.iterm2$", "^co\\.zeit\\.hyperterm$", "^co\\.zeit\\.hyper$", "^io\\.alacritty$", "^net\\.kovidgoyal\\.kitty$"],
                                    "type": "frontmost_application_if"
                                }],
                                "from": {"key_code": "right_arrow", "modifiers": {"mandatory": ["option"] } },
                                "to": [{"key_code": "escape"}, {"key_code": "f"} ],
                                "type": "basic"
                            }
                        ]
